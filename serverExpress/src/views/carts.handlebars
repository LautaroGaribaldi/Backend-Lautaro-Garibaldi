<div>
    <p>CARRITO ID {{id}}</p>
    <div class="cards">
        {{#each products}}
            <div class="cardsItems">
                <strong>nombre</strong>
                {{this.idProduct.title}}
                <strong>descripcion</strong>
                {{this.idProduct.description}}
                <strong>code</strong>
                {{this.idProduct.code}}
                <strong>precio</strong>
                {{this.idProduct.price}}
                <strong>cantidad</strong>
                {{this.quantity}}
            </div>
        {{/each}}
    </div>
    <form id="comprar" action="/api/carts/{{cart}}/purchase" method="GET">
        <button>Comprar</button>
    </form>
</div>